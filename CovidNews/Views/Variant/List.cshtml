@model IEnumerable<CovidNews.Models.VariantDto>

@{
    ViewBag.Title = "View";
}

<h2>View</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.VariantName)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>

            <td>
                @Html.DisplayFor(modelItem => item.VariantName)
            </td>

            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.VariantID }) |
                @Html.ActionLink("Details", "Details", new { id = item.VariantID })

            </td>
        </tr>
    }

</table>